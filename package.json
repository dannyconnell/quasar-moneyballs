    "pinia": "^2.0.11",
    "quasar": "^2.16.0",
    "sortablejs": "^1.15.2",
    "sortablejs-vue3": "^1.2.11",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.3",
    "@quasar/app-vite": "^1.9.0",
    "autoprefixer": "^10.4.2",
    "electron": "^31.0.1",
    "postcss": "^8.4.14"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
