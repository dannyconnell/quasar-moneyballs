{
  "name": "quasar-moneyballs",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Moneyballs",
  "author": "Danny Connell",
  "private": true,
  "scripts": {
    "icons": "icongenie generate -i app-icon.png --skip-trim --theme-color 00695c --splashscreen-icon-ratio 25",
    "dev": "quasar dev",
    "dev:mac": "quasar dev -m electron",
    "dev:ios": "quasar dev -m capacitor -T ios",
    "dev:android": "quasar dev -m capacitor -T android",
    "build": "quasar build",
    "build:mac": "quasar build -m electron",
    "build:win": "quasar build -m electron --target win32",
    "build:ios": "quasar build -m capacitor -T ios --ide",
    "build:android": "quasar build -m capacitor -T android --ide"
  },
  "dependencies": {
    "@capacitor/dialog": "^6.0.0",
